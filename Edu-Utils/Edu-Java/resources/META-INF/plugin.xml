<idea-plugin version="2">
  <id>com.jetbrains.edu.java</id>
  <name>Edu Java</name>
  <version>1.0.2</version>
  <vendor>JetBrains</vendor>

  <description><![CDATA[
      Educational plugin for Java programming language
    ]]></description>

  <change-notes>
  </change-notes>

  <idea-version since-build="144.3891"/>

  <depends>com.intellij.modules.lang</depends>
  <depends>com.jetbrains.edu.interactivelearning</depends>
  <depends>com.jetbrains.edu.intellij</depends>
  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <treeStructureProvider implementation="com.jetbrains.edu.utils.EduTreeStructureProvider" order="last"/>
  </extensions>

  <extensions defaultExtensionNs="Edu">
    <studyPluginConfigurator implementation="com.jetbrains.edu.java.EduJavaStudyToolWindowConfigurator"/>
  </extensions>
  <extensions defaultExtensionNs="com.jetbrains.edu.intellij">
    <courseConfigurator language="JAVA" implementationClass="com.jetbrains.edu.java.EduJavaCourseConfigurator"/>
  </extensions>

  <actions>
    <action id="EduJavaCheckAction" class="com.jetbrains.edu.java.EduJavaCheckAction" text="Edu Java Check"/>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>
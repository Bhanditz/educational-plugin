<idea-plugin version="2">
  <id>com.jetbrains.edu.intellij</id>
  <name>Edu IntelliJ</name>
  <version>1.0.6</version>
  <vendor>JetBrains</vendor>

  <description><![CDATA[
      Adds "Education" project group.
    ]]></description>

  <change-notes><![CDATA[
    ]]>
  </change-notes>

  <idea-version since-build="145"/>

  <depends>com.intellij.modules.lang</depends>
  <depends optional="true" config-file="android-dependencies.xml">com.intellij.modules.androidstudio</depends>

  <extensionPoints>
    <extensionPoint name="eduIntelliJProjectTemplate" interface="com.jetbrains.edu.intellij.EduIntelliJProjectTemplate"/>
    <extensionPoint qualifiedName="com.jetbrains.edu.intellij.courseConfigurator" beanClass="com.intellij.lang.LanguageExtensionPoint">
      <with attribute="implementationClass" implements="com.jetbrains.edu.intellij.EduCourseConfigurator"/>
    </extensionPoint>
  </extensionPoints>

  <extensions defaultExtensionNs="com.intellij">
   <projectTemplatesFactory implementation="com.jetbrains.edu.intellij.EduIntelliJTemplateFactory"/>
  </extensions>
</idea-plugin>